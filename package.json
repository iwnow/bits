{
  "name": "bits",
  "version": "0.9.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "nx serve crm --open --port=4300",
    "build": "nx build crm",
    "build:push": "npm run build && npm run docker:go",
    "test": "nx run-many --target=test",
    "test:core": "nx test crm-core --watch",
    "test:core:w": "nx test crm-core --watch",
    "prepare": "husky install",
    "release": "nx run workspace:version --releaseAs=minor --tagPrefix=v",
    "release:major": "nx run workspace:version --releaseAs=major --tagPrefix=v",
    "docker:build": "docker build -f ./docker/Dockerfile -t crm-ng:latest .",
    "docker:tag": "docker tag crm-ng:latest registry.beinteam.ru/crm-ng:latest",
    "docker:push": "docker push registry.beinteam.ru/crm-ng:latest",
    "docker:go": "npm run docker:build && npm run docker:tag && npm run docker:push",
    "migrate": "nx migrate latest && nx migrate --run-migrations"
  },
